{
  "name": "react-router-suspense",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "rm -rf ../../lib/* && tsc -p tsconfig.json && babel src --extensions \".es6,.js,.es,.mjs,.ts,.tsx\" --ignore src/**/*.spec.tsx,src/**/*.test.tsx --out-dir ../../lib --no-comments --copy-files",
    "build:watch": "yarn build --watch",
    "prepublish": "npm run build",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "tslint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagoleitedev/react-router-suspense.git"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/thiagoleitedev/react-router-suspense/issues"
  },
  "homepage": "https://github.com/thiagoleitedev/react-router-suspense#readme",
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-syntax-async-generators": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "7.7.2",
    "@testing-library/react": "^9.3.2",
    "@types/history": "^4.7.3",
    "@types/jest": "^24.0.23",
    "@types/react-relay": "^7.0.2",
    "@types/react-router": "^5.1.3",
    "@types/react-router-config": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "history": "^4.10.1",
